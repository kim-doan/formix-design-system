{
  "name": "@wdyj-lab/formix-design-system",
  "version": "0.6.0",
  "description": "Formix Design System Library",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./theme": {
      "import": "./dist/theme.js"
    },
    "./fonts": {
      "import": "./dist/fonts.js"
    },
    "./styles": "./dist/*.css",
    "./*.css": "./dist/*.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "design-system",
    "react",
    "components",
    "ui",
    "vanilla-extract"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wdyj-lab/formix-design-system.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.21.3",
    "@vanilla-extract/css": "^1.17.4",
    "color": "^5.0.0",
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "react": ">=16.8.0 || ^18.0.0 || ^19.0.0",
    "react-dom": ">=16.8.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@storybook/react-webpack5": "^9.0.17",
    "@types/color": "^4.2.0",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vanilla-extract/rollup-plugin": "^1.3.17",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rollup": "^4.9.1",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-node-externals": "^8.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "ts-loader": "^9.5.2",
    "typescript": "^5.3.3"
  }
}
